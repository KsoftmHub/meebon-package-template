{
  "name": "@meebon/meebon_package_template",
  "version": "1.0.6",
  "description": "",
  "author": "kajalanS <103587022+kajalanS@users.noreply.github.com>",
  "homepage": "https://github.com/KsoftmHub/meebon_package_template?tab=readme-ov-file#getting-started",
  "license": "MIT",
  "main": "dist/index.js",
  "type": "module",
  "types": "types",
  "directories": {
    "lib": "lib",
    "test": "tests",
    "types": "types",
    "bin": "bin"
  },
  "files": [
    "dist",
    "types",
    "test",
    "bin"
  ],
  "release": {
    "branches": [
      "master",
      "beta"
    ]
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KsoftmHub/meebon-package-template.git"
  },
  "bin": {
    "create-meebon-package": "./bin/cli.js"
  },
  "scripts": {
    "build:clean": "tsc --build --clean",
    "build": "tsc && jest",
    "test": "jest",
    "rm:node": "find . -name \"node_modules\" -type d -prune -exec rm -rf '{}' +",
    "rm:dist": "find . -name \"dist\" -type d -prune -exec rm -rf '{}' +",
    "clean": "pnpm run rm:node && pnpm run rm:dist",
    "semantic-release": "semantic-release"
  },
  "bugs": {
    "url": "https://github.com/KsoftmHub/meebon_package_template/issues"
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.5",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.19.1"
  }
}